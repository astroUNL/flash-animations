
package {
	
	public class Star {
		
		public var x:Number = 0;
		public var y:Number = 0;
		public var z:Number = 0;
		public var m:Number = 0;
		
		public function Star(x:Number, y:Number, z:Number, m:Number) {
			this.x = x;
			this.y = y;
			this.z = z;
			this.m = m;			
		}
		
	}
	
}
