
package {	
	public class Point3D {
		public var x:Number = 0;
		public var y:Number = 0;
		public var z:Number = 0;
		public var sx:Number = 0;
		public var sy:Number = 0;
		public function Point3D(x:Number=0, y:Number=0, z:Number=0, sx:Number=0, sy:Number=0) {
			this.x = x;
			this.y = y;
			this.z = z;
			this.sx = sx;
			this.sy = sy;
		}
	}
}
